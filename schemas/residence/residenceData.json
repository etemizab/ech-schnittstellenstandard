{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "schemas/shared/identifiers.json",
  "title": "residenceData",
  "type": "object",
  "properties": {
    "reportingMunicipality": { "type": "object",
      "properties": {
        "municipalityId": { "type": "string" },
        "municipalityName": { "type": "string" },
        "cantonAbbreviation": { "type": "string", "minLength": 2 }
      },
      "required": ["municipalityName"],
      "additionalProperties": false
    },
    "typeOfResidence": { "type": "integer", "enum": [1,2,3] },
    "arrivalDate": { "type": "string", "format": "date" },
    "comesFrom": { "type": "object" },
    "dwellingAddress": { "type": "object",
      "properties": {
        "EGID": { "type": "string" },
        "EWID": { "type": "string" },
        "typeOfHousehold": { "type": "integer", "enum": [0,1,2,3] },
        "movingDate": { "type": "string", "format": "date" },
        "validFrom": { "type": "string", "format": "date" }
      },
      "required": ["typeOfHousehold","validFrom"],
      "additionalProperties": false
    },
    "departureDate": { "type": "string", "format": "date" },
    "goesTo": { "type": "object" }
  },
  "required": ["reportingMunicipality","typeOfResidence","arrivalDate","dwellingAddress"],
  "additionalProperties": false
}
